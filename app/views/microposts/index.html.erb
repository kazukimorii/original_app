<% if user_signed_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <h1>
          <ul class="users">
            <%= render @users %>
          </ul>
        </h1>
      </section>
    </aside>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>糖質制限を始めよう</h1>

    <h2>
      糖質制限についてもっと知りたければ下記からユーザー登録!
    </h2>

    <%= link_to "新規登録", new_user_registration_path, class: "btn btn-lg btn-primary" %>
  </div>
<% end %>